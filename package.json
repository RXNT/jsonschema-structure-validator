{
  "name": "jsonschema-structure-validator",
  "version": "0.0.1",
  "description": "Simple jsonschema structure validator",
  "private": false,
  "author": "mavarazy@gmail.com",
  "scripts": {
    "build:lib": "rimraf lib && cross-env NODE_ENV=production babel -d lib/ src/",
    "build:dist": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack.config.js --optimize-minimize",
    "cs-check": "prettier -l $npm_package_prettierOptions '{src,test}/**/*.js'",
    "cs-format": "prettier $npm_package_prettierOptions '{src,test}/**/*.js' --write",
    "dist": "npm run build:lib && npm run build:dist",
    "lint": "eslint src test",
    "precommit": "lint-staged",
    "publish-to-npm": "npm run dist && npm version patch && npm publish",
    "tdd": "jest --watchAll",
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "prettierOptions": "--jsx-bracket-same-line --trailing-comma es5 --semi",
  "lint-staged": {
    "{src,test}/**/*.js": [
      "npm run lint",
      "npm run cs-format",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "engineStrict": false,
  "engines": {
    "npm": "^2.14.7",
    "node": ">=6"
  },
  "dependencies": {
  },
  "devDependencies": {
    "atob": "^2.0.3",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.1.1",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "exit-hook": "^1.1.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "gh-pages": "^1.0.0",
    "has-flag": "^2.0.0",
    "html": "1.0.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "jsdom": "^11.1.0",
    "lint-staged": "^4.0.0",
    "prettier": "^1.5.2",
    "regenerator-runtime": "^0.10.5",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RxNT/jsonschema-structure-validator.git"
  },
  "keywords": [
    "jsonschema validator"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/RxNT/jsonschema-structure-validator#readme"
}
